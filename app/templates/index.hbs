{{#if model.length}}
	<section id="main">
		<ul id="todo-list" class="todo-list">
		{{#each model as |todo|}}
			<li>
				<div class="view">
					<input type="checkbox" class="toggle">
					<label>{{todo.title}}</label>
					<button class="destroy"></button>
				</div>
			</li>
		{{/each}}
	</ul>
	</section>
{{/if}}
